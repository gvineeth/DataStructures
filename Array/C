#include <stdio.h>
int main(){
    int a[10]; // create array of size 10
    a[0]=1;    // set value
    a[2]=2;
    a[5/4]=3;
    printf("%d, %d, %d, %d",a[0],a[1],a[6],a[1000]); //1, 3, -1710541408, 0
    
    char arr1[] = {'p', 'r', 'o', 'g', 'r', 'a', 'm'};
    printf("\n%c",arr1[0]); // p
    
    arr1[0]='C';
    printf("\n%c",arr1[0]); // C
    
    int arr[2] = { 10, 20, 30, 40, 50 }; // compiles but warns


    return 0; 
}
